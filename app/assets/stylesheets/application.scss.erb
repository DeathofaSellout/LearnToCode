@import "materialize";

.footer{
  font-family: 'Libre Barcode 39 Text', cursive;
  background-color: black;
  color: #648880;
  text-align: center;
  position: absolute;
  left: 0 ; right: 0; bottom: 0;
  //height:[footer-height-20px];
  height: 30px;
  margin-top: 10px;
}
h4 {
  font-size: 30px;
}
.logo {
  float: left;
  font-size: 48px;
  font-weight: 100;
  letter-spacing: .5px;
  line-height: 44px;
  text-transform: uppercase;
  color: #648880;
  margin: 5px;
  margin-top: 10px;
}
nav{
  background-color: black !important;
}
.nav-wrapper {
  background-color: black !important;
  text-align: right;
  height: 50px;
}
.nav-wrapper-two {
  background-color: white !important;
  text-align: right;
  height: 70px;
}
#intro {
  margin: 5%;
  width: 90%;
  height: 80% !important;
  background-color: black;
  border-radius: 30px;
  text-align: center;
  border-style: solid;
  color: white;
  padding: 10px;
}
html { height: 100%; width: 100%}
body {
  background-color: black;
  min-height:85%;
  position:relative;
  // padding-bottom:[footer-height];
